{
  "name": "digital-clock-react",
  "description": "This NPM package presents a digital-clock component for ReactJS applications, it offers intuitive and visually appealing clock feature with easy integration capabilities.",
  "author": "afek-sakaju",
  "homepage": "http://Afek-Sakaju.github.io/digital-clock-react/",
  "keywords": [
    "clock",
    "digital-clock",
    "time",
    "react",
    "component"
  ],
  "version": "1.1.7",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Afek-Sakaju/digital-clock-react"
  },
  "scripts": {
    "start": "react-scripts start",
    "build-and-publish": "npm run build && npm run publish",
    "publish": "npm publish",
    "build": "del dist && babel src/ --out-dir dist --copy-files",
    "lint": "eslint --ext .jsx --ext .js .",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "predeploy": "react-scripts build",
    "deploy": "gh-pages -d build",
    "build-jsdocs": "jsdoc src/utils/functions.js -d js-docs"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
